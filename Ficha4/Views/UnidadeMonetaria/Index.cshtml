@model IEnumerable<Ficha4.Models.UnidadeMonetaria>

<h3>Cenas </h3> 
<table>
    <thead>
        <tr>
            <th>Código</th>
            <th>Valor Euro</th>
            <th>Data</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
       {
           <tr>
               <td>@x.Codigo</td>
               <td>@x.Valor</td>
               <td>@x.Data</td>
           </tr>
       }
    </tbody>
</table>
